@prefix : <http://example.org/nanodesk/example/read/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix pav: <http://purl.org/pav/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix ex: <http://example.org/> .
@prefix orcid: <http://orcid.org/> .

:Head {
	: np:hasAssertion :assertion ;
		np:hasProvenance :provenance ;
		np:hasPublicationInfo :pubinfo ;
		a np:Nanopublication .
}

:assertion {
	orcid:ORCID-ID-HERE ex:hasRead <http://dx.doi.org/DOI-HERE> .
}

:provenance {
	:assertion prov:wasAttributedTo orcid:ORCID-ID-HERE .
}

:pubinfo {
	: dc:created "TIMESTAMP-HERE"^^xsd:dateTime ;
		pav:createdBy orcid:ORCID-ID-HERE .
}
