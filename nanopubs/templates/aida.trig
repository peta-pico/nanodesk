@prefix : <http://purl.org/np/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix pav: <http://purl.org/pav/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix npx: <http://purl.org/nanopub/x/> .
@prefix hycl: <http://purl.org/petapico/o/hycl#> .
@prefix orcid: <http://orcid.org/> .

:Head {
	: np:hasAssertion :assertion ;
		np:hasProvenance :provenance ;
		np:hasPublicationInfo :pubinfo ;
		a np:Nanopublication .
}

:assertion {
    <http://doi.org/DOI-HERE> hycl:claims <http://purl.org/aida/AIDA-SENTENCE-HERE> .
}

:provenance {
	:assertion prov:wasAttributedTo orcid:ORCID-ID-HERE .
}

:pubinfo {
	: dc:created "TIMESTAMP-HERE"^^xsd:dateTime ;
		pav:createdBy orcid:ORCID-ID-HERE ;
		prov:wasAttributedTo <https://github.com/peta-pico/nanodesk> .
	: a npx:ExampleNanopub . # remove this line once we are done with testing
}
